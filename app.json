{
  "expo": {
    "name": "AllIn",
    "slug": "AllIn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon2.png",
    "splash": {
      "image": "./app/assets/splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#e63e4b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "Gym App - getting started with react native "
  }
}
